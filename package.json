{
    "name": "utari-client",
    "version": "0.0.0",
    "private": true,
    "author": "PoolOfDeath20",
    "license": "MIT",
    "proxy": "http://localhost:5000",
    "scripts": {
        "start": "yarn type:check & yarn build",
        "start:watch": "nodemon",
        "build": "rm -rf build && cp -R public build && node script/esbuild.js && node script/terser.js",
        "type:check": "tsc --pretty --skipLibCheck --noEmit",
        "test": "jest",
        "code-cov": "jest --coverage --coverageDirectory='coverage'"
    },
    "dependencies": {
        "@react-google-maps/api": "^2.7.0",
        "dotenv": "^11.0.0",
        "granula-string": "^1.0.3",
        "leaflet": "^1.7.1",
        "parse-dont-validate": "^1.0.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-google-maps": "^9.4.5",
        "react-helmet-async": "^1.1.2",
        "react-icons": "^4.2.0",
        "react-leaflet": "^3.2.5",
        "react-router-dom": "^5.3.0",
        "react-spinners": "^0.11.0",
        "styled-components": "^5.3.1",
        "web-vitals": "^1.0.1"
    },
    "devDependencies": {
        "@babel/preset-typescript": "^7.16.0",
        "@types/jest": "^27.0.2",
        "@types/leaflet": "^1.7.8",
        "@types/node": "^17.0.8",
        "@types/react": "^17.0.37",
        "@types/react-dom": "^17.0.11",
        "@types/react-router-dom": "^5.3.2",
        "@types/styled-components": "^5.1.15",
        "esbuild": "^0.14.11",
        "html-minifier-terser": "^7.0.0-alpha.1",
        "nodemon": "^2.0.15",
        "terser": "^5.10.0",
        "ts-jest": "^27.0.7",
        "typescript": "^4.1.2"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
